{
  "name": "Minute",
  "description": "Using Interledger, automatically support content creators.",
  "version": "2.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "res/ilp_logo_128.png"
  },
  "browser_action": {
    "default_icon": "res/ilp_logo.png",
    "default_popup": "static/popup.html"
  },
  "background": {
    "page": "static/background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["content.js"],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "key_comment": "https://stackoverflow.com/a/37318349/11587288",
  "key": "",
  "permissions": [ "tabs", "http://localhost:7768/", "http://localhost:7768/" ]
}
