{
  "$schema": "../coil-monorepo-upkeep/resources/package-json-schema-nested-overrides.json",
  "name": "@coil/local-server",
  "version": "0.0.0",
  "keywords": [
    "ilp",
    "web-monetization"
  ],
  "homepage": "https://github.com/coilhq/web-monetization-projects/tree/main/packages/coil-local-server",
  "repository": {
    "type": "git",
    "url": "git@github.com:coilhq/web-monetization-projects.git"
  },
  "license": "Apache-2.0",
  "author": "Coil Team <info@coil.com>",
  "main": "./build",
  "types": "./build",
  "scripts": {
    "build:ts": "tsc --build tsconfig.build.json",
    "build:ts:verbose": "pnpm build:ts --verbose",
    "build:ts:watch": "pnpm build:ts --watch",
    "clean:build": "rimraf build",
    "dev:graphql-watch": "pnpm generate-graphql --watch",
    "dev:server": "nodemon src/index.ts",
    "format": "pnpm prettier && LINT_FIX=1 pnpm lint:all --fix --quiet",
    "generate-code": "pnpm generate-graphql && pnpm prettier && pnpm lint:all --fix",
    "generate-graphql": "graphql-codegen --config codegen.yml",
    "lint": "eslint --cache --cache-location ../../node_modules/.cache/eslint",
    "lint:all": "pnpm lint 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "precommit": "echo lint-staged runs from root",
    "prettier": "prettier --write '*.{ts,tsx,js,html,jsx,md}' '{src,test}/**/*.{ts,tsx,js,html,jsx,md}'",
    "test": "PROJECT_JEST=1 jest --passWithNoTests",
    "test:coverage": "pnpm run test --verbose --coverage",
    "test:e2e": "pnpm run test --config jest-e2e.config.cjs",
    "test:e2e:coverage": "pnpm test:coverage --config jest-e2e.config.cjs",
    "tsnodeenv": "NODE_OPTIONS=\"${NODE_OPTIONS:-} --require tsconfig-paths/register\" TS_NODE_PROJECT=\"../../tsconfig.cjs.json\"",
    "upkeep": "cd ../.. && pnpm upkeep"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@apollo/utils.logger": "^2.0.0",
    "@coil/privacypass-sjcl": "0.0.0",
    "@coil/puppeteer-utils": "0.0.0",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@interledger/stream-receiver": "^0.3.3-alpha.1",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^3.7.0",
    "body-parser": "^1.20.0",
    "compose-middleware": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "express-jwt": "^8.0.0",
    "express-ws": "^5.0.2",
    "graphql": "^16.4.0",
    "graphql-tools": "^9.0.0",
    "ilp-plugin-mini-accounts": "^4.2.0",
    "ilp-protocol-stream": "2.7.2-alpha.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "inversify-logger-middleware": "^3.1.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@abraham/reflection": "0.12.0",
    "@babel/core": "7.22.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.22.3",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/preset-env": "7.22.4",
    "@babel/preset-react": "7.22.3",
    "@babel/preset-typescript": "7.21.5",
    "@coil/client": "0.0.0",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-resolvers": "3.2.1",
    "@swc/core": "1.3.64",
    "@swc/jest": "0.2.26",
    "@types/body-parser": "1.19.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.16.16",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@webmonetization/types": "0.0.0",
    "JSON2016": "1.0.0",
    "babel-plugin-parameter-decorator": "1.0.16",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "regenerator-runtime": "0.13.11",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "webpack": "5.85.1",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.0",
    "webpack-merge": "5.9.0"
  }
}
